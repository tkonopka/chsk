{
    "name": "@chsk/xy",
    "version": "0.9.0-alpha.0",
    "author": "Tomasz Konopka",
    "description": "Components for charts with two cartesian axes with continuous scales",
    "keywords": [
        "react",
        "visualization",
        "data visualization",
        "chart",
        "charts",
        "scatter",
        "scatter-chart",
        "line",
        "line-chart",
        "histogram",
        "histogram-chart",
        "density",
        "density-chart",
        "density-map"
    ],
    "exports": {
        "import": "./dist/chsk-xy.es.js",
        "require": "./dist/chsk-xy.umd.js"
    },
    "type": "module",
    "module": "./dist/chsk-xy.es.js",
    "typings": "./dist/types/index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/tkonopka/chsk.git",
        "directory": "packages/xy"
    },
    "scripts": {
        "lint": "eslint --max-warnings 0 ./{src,tests,stories}/**/*{js,ts,tsx}",
        "test": "jest --json --outputFile tests/jest.json",
        "test-coverage": "jest --coverage",
        "build": "PACKAGE=xy rollup -c ../../rollup.config.js && tsc"
    },
    "dependencies": {
        "@chsk/core": "^0.9.0-alpha.0",
        "framer-motion": ">=7.0.0",
        "lodash": "^4.17.21"
    },
    "peerDependencies": {
        "react": ">= 17.0.2",
        "react-dom": ">= 17.0.2"
    },
    "license": "MIT",
    "files": [
        "README.md",
        "LICENSE.md",
        "dist/"
    ]
}

{
    "name": "@chsk/band",
    "version": "0.7.0",
    "author": "Tomasz Konopka",
    "description": "Components for charts with one categorical axis and one continuous axis",
    "keywords": [
        "react",
        "visualization",
        "data visualization",
        "chart",
        "charts",
        "bar",
        "bar-chart",
        "box-plot",
        "quantile",
        "quantile-chart",
        "strip",
        "strip-chart",
        "schedule",
        "schedule-chart",
        "violin",
        "violin-chart",
        "dendrogram",
        "dendrogram-chart"
    ],
    "exports": {
        "import": "./dist/chsk-band.es.js",
        "require": "./dist/chsk-band.umd.js"
    },
    "type": "module",
    "module": "./dist/chsk-band.es.js",
    "typings": "./dist/types/index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/tkonopka/chsk.git",
        "directory": "packages/band"
    },
    "scripts": {
        "lint": "eslint --max-warnings 0 ./{src,tests,stories}/**/*{js,ts,tsx}",
        "test": "jest --json --outputFile tests/jest.json",
        "test-coverage": "jest --coverage",
        "build": "PACKAGE=band rollup -c ../../rollup.config.js && tsc"
    },
    "dependencies": {
        "@chsk/core": "^0.7.0",
        "framer-motion": ">=7.0.0"
    },
    "peerDependencies": {
        "react": ">= 17.0.2 < 19.0.0",
        "react-dom": ">= 17.0.2 < 19.0.0"
    },
    "license": "MIT",
    "files": [
        "README.md",
        "LICENSE.md",
        "dist/"
    ]
}
